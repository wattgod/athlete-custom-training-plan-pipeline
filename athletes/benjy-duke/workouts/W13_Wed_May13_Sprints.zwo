    <Warmup Duration="1350" PowerLow="0.50" PowerHigh="0.75"/>
    <IntervalsT Repeat="7" OnDuration="30" OnPower="1.55" CadenceLow="85" CadenceHigh="95" OffDuration="120" OffPower="0.50"/>
    <Cooldown Duration="600" PowerLow="0.75" PowerHigh="0.50"/>
  </workout>
